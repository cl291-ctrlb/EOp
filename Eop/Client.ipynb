   "source": [
    "import socket\n",
    "\n",
    "client = socket.socket()\n",
    "client.connect((\"localhost\", 9999))\n",
    "\n",
    "client.send(\"Hello Server!\".encode())\n",
    "data = client.recv(1024).decode()\n",
    "print(\"Server says:\", data)\n",
    "\n",
    "client.close()\n"
   ]
